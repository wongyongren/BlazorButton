@page "/button2"

<h3>Button2</h3>
<h1>@checkedValue</h1>

<div class="toggle-wrapper">
    <label class="toggle">
        <input type="checkbox"
               @bind="checkedValue" />
        <span class="toggler round"></span>
    </label>
</div>

@code {
    private Boolean checkedValue = true;
}